#include <cctype>
#include <iostream>
#include <cstring>
#include <cstdio>

using namespace std;

int main()
{
    char str[100];
    cin>>str;

  

    for (int i=0; i<strlen(str); i++)
    if(islower(str[i]))
        putchar(toupper(str[i]));
    else
        putchar(tolower(str[i]));
    
    return 0;
}
